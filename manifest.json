{
    "manifest_version": 3,
    "name": "Facebook Deadly",
    "description": "Extension to remove Facebook news feed",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "images/fb-deadly-logo.png"
    },
    "content_scripts": [
    {
      "js": ["scripts/fb-deadly.js"],
      "matches": [
        "https://www.facebook.com/*"
      ]
    }
  ]
  }